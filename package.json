{
  "name": "pwa-starter-kit",
  "version": "",
  "flat": "true",
  "contributors": [
    "The Polymer Authors"
  ],
  "license": "https://polymer.github.io/LICENSE.txt",
  "scripts": {
    "start": "polymer serve",
    "build": "polymer build",
    "rename": "LC_ALL=C find build -type f -exec sed -i '' s/node_modules/node_assets/g {} + && mv build/es5-bundled/node_modules build/es5-bundled/node_assets && mv build/es6-bundled/node_modules build/es6-bundled/node_assets && mv build/esm-bundled/node_modules build/esm-bundled/node_assets",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "mocha test/integration --timeout=10000",
    "test:unit": "wct --module-resolution=node --npm -l chrome",
    "test:regenerate_screenshots": "mocha test/integration/screenshots-baseline/regenerate.js --timeout=10000"
  },
  "dependencies": {
    "@polymer/lit-element": "^0.2.2",
    "@webcomponents/webcomponentsjs": "^1.0.0",
    "pwa-helpers": "polymerlabs/pwa-helpers",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "axe-core": "^3.0.0",
    "chai": "^4.1.2",
    "mocha": "^5.0.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.7.0-pre.4",
    "puppeteer": "^1.0.0",
    "wct-browser-legacy": "0.0.1-pre.11",
    "web-component-tester": "^6.6.0-pre.3"
  }
}
